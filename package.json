{
  "name": "react-lazy-load-image-observer",
  "version": "0.0.4",
  "description": "The LazyLoadImage component allows you to lazy load images, or background-images, and set the observed element.",
  "author": "Jon K. Wheeler",
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonkwheeler/react-lazy-load-image-observer.git"
  },
  "publishConfig": {
    "scope": "jonkwheeler",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "dirBuild": "./dist",
    "dirSrc": "./src",
    "dirStorybook": "./.storybook",
    "dirStorybookBuild": "./.out",
    "portStorybook": "6876"
  },
  "scripts": {
    "build:commonjs": "yarn nuke:commonjs && cross-env NODE_ENV=production BABEL_ENV=commonjs babel $npm_package_config_dirSrc --out-dir $npm_package_config_dirBuild --extensions \".ts,.tsx,.js,.jsx\" --ignore \"**/*.stories.js\",\"**/story.js\",\"**/stories\",\"**/*.test.js\",\"test.js\",\"**/test\",\"**/*.md\",\"**/docs\",*/node_modules/styled-components --source-maps inline",
    "build:export-indexes": "node ./scripts/exportIndexes.js",
    "build:export-src-index": "node ./scripts/exportSrcIndex.js",
    "build:types": "tsc --project tsconfig.types.json && tscpaths --project tsconfig.types.json --src $npm_package_config_dirSrc",
    "build": "yarn build:commonjs",
    "dev": "yarn start:storybook",
    "nuke:all:install:dev": "yarn nuke:all && yarn && yarn dev",
    "nuke:all:install": "yarn nuke:all && yarn",
    "nuke:all": "rm -rf node_modules .cache $npm_package_config_dirBuild yarn.lock yarn-error.log package-lock.json",
    "nuke:build:rebuild": "yarn nuke:build && yarn build",
    "nuke:build": "rm -rf $npm_package_config_dirBuild",
    "nuke:commonjs": "rm -rf $npm_package_config_dirBuild",
    "prepublishOnly": "yarn nuke:build:rebuild",
    "release": "yarn release-it",
    "start:storybook": "start-storybook -p $npm_package_config_portStorybook -c $npm_package_config_dirStorybook",
    "storybook": "yarn start:storybook"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.4",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.13.0",
    "fork-ts-checker-webpack-plugin": "^1.3.3",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "release-it": "^12.0.1",
    "storybook": "^1.0.0",
    "ts-loader": "^6.0.0",
    "tscpaths": "^0.0.7",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0"
  },
  "bugs": {
    "url": "https://github.com/jonkwheeler/react-lazy-load-image-observer/issues"
  },
  "homepage": "https://github.com/jonkwheeler/react-lazy-load-image-observer#readme",
  "keywords": [
    "react",
    "lazy",
    "load",
    "image",
    "observer"
  ]
}
